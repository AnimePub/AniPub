<script src="https://vjs.zencdn.net/8.16.1/video.min.js"></script>
<script>
  /* Nav */
  function toggleNav() {
    const n = document.getElementById('mobnav');
    const h = document.getElementById('ham');
    const o = n.classList.toggle('open');
    h.classList.toggle('open', o);
  }
  function closeNav() {
    document.getElementById('mobnav').classList.remove('open');
    document.getElementById('ham').classList.remove('open');
  }
  window.addEventListener('resize', () => { if (window.innerWidth > 768) closeNav(); });

  /* Scroll to section */
  function sid(id) {
    const el = document.getElementById(id);
    if (el) el.scrollIntoView({ behavior: 'smooth', block: 'start' });
  }

  /* Search */
  function doSearch() {
    const q = document.getElementById('hs').value.trim();
    if (q) window.location.href = '/search/q?query=' + encodeURIComponent(q);
  }
  function qs(t) {
    document.getElementById('hs').value = t;
    document.getElementById('hs').focus();
  }
  document.getElementById('hs').addEventListener('keydown', e => { if (e.key === 'Enter') doSearch(); });

  /* Video side list */
  function pickVid(card) {
    document.querySelectorAll('.mini-card').forEach(c => c.classList.remove('on'));
    card.classList.add('on');
  }

  /* Dialog backdrop click */
  document.getElementById('dlg').addEventListener('click', function(e) {
    if (e.target === this) this.close();
  });

  /* Video.js init */
  if (typeof videojs !== 'undefined') {
    videojs('featVid');
  }

  /* Scroll reveal */
  const rvEls = document.querySelectorAll('.rv');
  const ro = new IntersectionObserver(entries => {
    entries.forEach(e => {
      if (e.isIntersecting) { e.target.classList.add('vis'); ro.unobserve(e.target); }
    });
  }, { threshold: 0.1 });
  rvEls.forEach(el => ro.observe(el));
</script>

